{
  "manifest_version": 3,
  "name": "Job Extractor",
  "version": "1.0",
  "description": "Extract job info from the current page",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "host_permissions": ["<all_urls>"]
}